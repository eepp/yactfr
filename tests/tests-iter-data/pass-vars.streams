trace {
    major = 1;
    minor = 8;
    byte_order = be;
    packet.header := struct {
        enum : u8 {
            A = 1 ... 5,
            A = 9 ... 17,
            B = 18,
            C = 7,
        } tag;
        variant <tag> {
            string A;
            u32 B;
            u8 C;
        } v;
        enum : i8 {
            A, B, C,
        } ph_tag;
        variant <ph_tag> {
            string A;
            u32 B;
            u8 C;
        } v_rel;
    };
};

stream {
    packet.context := struct {
        enum : u8 {
            A, B, C,
        } pc_tag;
        variant <pc_tag> {
            string A;
            u32 B;
            u8 C;
        } v_rel;
        variant <trace.packet.header.ph_tag> {
            string A;
            u32 B;
            u8 C;
        } v_abs;
    };
    event.header := struct {
        enum : u8 {
            A, B, C,
        } eh_tag;
        variant <eh_tag> {
            string A;
            u32 B;
            u8 C;
        } v_rel;
        variant <stream.packet.context.pc_tag> {
            string A;
            u32 B;
            u8 C;
        } v_abs;
    };
    event.context := struct {
        enum : u8 {
            A, B, C,
        } e1c_tag;
        variant <e1c_tag> {
            string A;
            u32 B;
            u8 C;
        } v_rel;
        variant <stream.event.header.eh_tag> {
            string A;
            u32 B;
            u8 C;
        } v_abs;
    };
};

event {
    name = test;
    context := struct {
        enum : u8 {
            A, B, C,
        } e2c_tag;
        variant <e2c_tag> {
            string A;
            u32 B;
            u8 C;
        } v_rel;
        variant <stream.event.context.e1c_tag> {
            string A;
            u32 B;
            u8 C;
        } v_abs;
    };
    fields := struct {
        enum : u8 {
            A, B, C,
        } ep_tag;
        variant <ep_tag> {
            string A;
            u32 B;
            u8 C;
        } v_rel;
        variant <event.context.e2c_tag> {
            string A;
            u32 B;
            u8 C;
        } v_abs;
    };
};
