/* CTF 1.8 */

trace {
  major = 1;
  minor = 8;
  byte_order = le;
};

event {
  name = event;

  fields := struct {
    typealias integer {size = 8; signed = false;} := uint8;

    enum : uint8 {
      x, y
    } tag;

    variant <tag> {
      struct {
        string len;
        string seq[len];
      } x;

      string y;
    } var;
  };
};
