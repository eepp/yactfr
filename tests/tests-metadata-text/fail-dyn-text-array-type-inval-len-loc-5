/* CTF 1.8 */

trace {
  major = 1;
  minor = 8;
  byte_order = le;
};

event {
  name = event;

  fields := struct {
    typealias integer {size = 8; signed = false;} := uint8;

    enum : uint8 {
      x, y
    } tag;

    variant <tag> {
      struct {
        uint8 len2;
      } x;

      struct {
        string len2;
      } y;
    } len1;

    integer { size = 8; encoding = utf8; } seq[len1.len2];
  };
};
